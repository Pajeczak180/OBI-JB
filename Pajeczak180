    <script>
        var dolary = 273
        document.getElementById("wynik2").textContent = `Zebraliśmy już ${dolary}/1000 zł (${dolary/1000*100}%)`
        function wplac(){
            m4.style.visibility = `hidden`
            a4.style.visibility = `hidden`
            b4.style.visibility = `visible`
        }
        function wplac2(){
            d4.style.visibility = `visible`
            a4.style.visibility = `visible`
            b4.style.visibility = `hidden`
            var kwota = Number(document.getElementById("zl").value)
            dolary += kwota
            document.getElementById("wynik2").textContent = `Zebraliśmy już ${dolary}/1000 zł (${dolary/1000*100}%)`
        }
        function nie(){
            m4.style.visibility = `hidden`
            a4.style.visibility = `hidden`
            c4.style.visibility = `visible`
        }
    </script>
